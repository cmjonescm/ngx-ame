{
  "name": "cjsoftware",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmjonescm/ngx-ame.git"
  },
  "bugs": {
    "url": "https://github.com/cmjonescm/ngx-ame/issues"
  },
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "postinstall": "ngcc --properties es2015 browser module main",
    "copy-readme": "copy .\\README.md .\\dist\\libs\\foundation",
    "copy-license": "copy .\\LICENSE .\\dist\\libs\\foundation",
    "copy-changelog": "copy .\\Changelog.md .\\dist\\libs\\foundation",
    "copy-files": "run-s copy-readme copy-license",
    "commit-git": "git commit -m \"build: adds new release\" && git push origin && git push origin --tags",
    "bump-version:major": "npm version major --force && cd libs/foundation && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/foundation && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/foundation && npm version patch --force",
    "build:lib-pack": "ng build foundation && npm run copy-files &&  cd dist/libs/foundation && npm pack",
    "pre-publish:minor": "run-s bump-version:minor build:lib-pack commit-git",
    "pre-publish:major": "run-s bump-version:major build:lib-pack commit-git",
    "pre-publish:patch": "run-s bump-version:patch build:lib-pack commit-git"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@nrwl/angular": "^12.10.0",
    "@nrwl/cli": "12.10.0",
    "@nrwl/eslint-plugin-nx": "12.10.0",
    "@nrwl/jest": "12.10.0",
    "@nrwl/linter": "12.10.0",
    "@nrwl/tao": "12.10.0",
    "@nrwl/workspace": "12.10.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "ng-packagr": "^12.1.1",
    "ngx-deploy-npm": "^3.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  }
}
